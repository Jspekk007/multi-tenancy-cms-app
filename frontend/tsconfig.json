{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": ".nuxt/tsc",
    "types": ["vite/client"],
    "strict": true,
    "noEmit": true,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "lib": ["esnext", "dom"],
    "jsx": "preserve",
    "skipLibCheck": true
  },
  "include": ["**/*.ts", "**/*.vue", "**/*.d.ts", "*.js"],
  "exclude": ["node_modules", ".nuxt", "dist"]
}
