{
  "name": "backend",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "prisma:generate": "prisma generate --schema ./prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema ./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema ./prisma/schema.prisma",
    "prisma:db-push": "prisma db push --schema ./prisma/schema.prisma",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts,.d.ts",
    "lint:fix": "eslint src --ext .ts,.d.ts --fix"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "@supabase/supabase-js": "^2.57.4",
    "@trpc/client": "^11.5.1",
    "@trpc/react-query": "^11.5.1",
    "@trpc/server": "^11.5.1",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "pino": "^9.11.0",
    "pino-http": "^10.5.0",
    "prisma": "^6.16.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.5.2",
    "dotenv-cli": "^10.0.0",
    "nodemon": "^3.1.10",
    "pino-pretty": "^13.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2"
  }
}
